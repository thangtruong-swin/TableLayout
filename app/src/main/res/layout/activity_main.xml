<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/myTable"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/imagetarneitshoppingcenter"
            android:layout_width="wrap_content"
            android:layout_height="95dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/tarneitshoppingcenter" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRowTextTarneitShoppingcenter"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:id="@+id/title_tarneit_shoping_center"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/title_tarneit_shoping_center"
            android:textSize="18sp"
            android:textColor="@color/black"
            />
    </TableRow>

    <TableRow android:id="@+id/table_simpleRatingBar">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <RatingBar
                android:id="@+id/simpleRatingBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:isIndicator="true"
                android:numStars="5"
                android:rating="4" />
        </RelativeLayout>
    </TableRow>

    <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
        >
                <ImageView
                    android:id="@+id/imagebunningwarehouse"
                    android:layout_width="wrap_content"
                    android:layout_height="95dp"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/bunningwarehouse" />
    </TableRow>
    <TableRow
        android:id="@+id/tableRowTextbunningwarehouse"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:id="@+id/title_bunningwarehouse"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/title_bunning_warehouse"
            android:textSize="18sp"
            android:textColor="@color/black"
            />
    </TableRow>
    <TableRow>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <RatingBar
                android:id="@+id/simpleRatingBar2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:isIndicator="true"
                android:numStars="5"
                android:rating="4.5" />
        </RelativeLayout>
    </TableRow>

<!--    Chemest Ware house-->
    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <ImageView
            android:id="@+id/imagetarneit_staion"
            android:layout_width="wrap_content"
            android:layout_height="95dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/tarneit_staion" />
    </TableRow>
    <TableRow
        android:id="@+id/tableRowTextimagechemistwarehousestore"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:id="@+id/title_chemistwarehousestore"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/image_tarneit_staion"
            android:textSize="18sp"
            android:textColor="@color/black"
            />
    </TableRow>
    <TableRow>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <RatingBar
                android:id="@+id/simpleRatingBar3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:isIndicator="true"
                android:numStars="5"
                android:rating="3.5" />
        </RelativeLayout>
    </TableRow>



<!--Village Cinemas-->
    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <ImageView
            android:id="@+id/imagevillagecinemas"
            android:layout_width="wrap_content"
            android:layout_height="95dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/villagecinemas" />
    </TableRow>
    <TableRow
        android:id="@+id/tableRowTextimagevillagecinemas"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:id="@+id/title_villagecinemas"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/title_villagecinemas"
            android:textSize="18sp"
            android:textColor="@color/black"
            />
    </TableRow>
    <TableRow>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <RatingBar
                android:id="@+id/simpleRatingBar4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:isIndicator="true"
                android:numStars="5"
                android:rating="3.5" />
        </RelativeLayout>
    </TableRow>

</TableLayout>